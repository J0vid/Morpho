    foreach (>= 1.4.0),
    Matrix (>= 1.0-1),
    parallel,
    yaImpute,
    doParallel (>= 1.0.6),
    colorRamps,
    Rcpp,
    graphics,
    grDevices,
    methods, 
    stats,
    utils
LinkingTo: Rcpp, RcppArmadillo (>= 0.4)
